            nameFilter="plugin.xml"
            id="org.eclipse.pde.ui.UpdateClasspathAction1">
         <filter
               name="projectNature"
               value="org.eclipse.jdt.core.javanature">
         </filter>
         <action
               label="%UpdateClasspathAction.label"
               helpContextId="org.eclipse.pde.doc.user.update_classpath"
