               menubarPath="additions"
               enablesFor="+"
               id="org.eclipse.pde.ui.OpenDependenciesAction2">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            adaptable="true"
            id="org.eclipse.pde.UpdateClasspathAction3">
         <filter
               name="projectNature"
               value="org.eclipse.pde.PluginNature">
         </filter>
         <action
