               menubarPath="additions"
               enablesFor="+"
               id="org.eclipse.pde.ui.OpenDependenciesAction1">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="fragment.xml"
            id="org.eclipse.pde.ui.UpdateClasspathAction2">
         <filter
               name="projectNature"
