<?xml version="1.0" encoding="UTF-8"?>
<fragment
   id="org.eclipse.help.ui.win32"
   name="%help_system_plugin_name_win32"
   version="2.1.0"
   provider-name="%providerName"
   plugin-id="org.eclipse.help.ui"
   plugin-version="2.1.0"
   match="compatible">

   <runtime>
      <library name="helpworkbenchwin32.jar">
         <export name="*"/>
      	 <packages prefixes="org.eclipse.help.ui.internal.browser.win32"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
   </requires>


<!-- WebBrowsers -->
   <extension
         point="org.eclipse.help.browser">
      <browser
            factoryclass="org.eclipse.help.ui.internal.browser.win32.IEFactory"
            name="%internet_explorer"
            id="org.eclipse.help.ui.iexplorer">
      </browser>
   </extension>
</fragment>
