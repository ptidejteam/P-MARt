<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="org.eclipse.pde.runtime"
   name="%name"
   version="2.1.0"
   provider-name="%provider-name"
   class="org.eclipse.pde.internal.runtime.PDERuntimePlugin">

<!-- Runtime -->
   <runtime>
      <library name="pdert.jar">
         <export name="*"/>
         <packages prefixes="org.eclipse.pde.internal.runtime"/>
      </library>
   </runtime>
<!-- Required plugins -->
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.resources"/>
   </requires>


<!-- Extensions -->
<!-- Extension points -->
   <extension
         point="org.eclipse.ui.views">
      <category
            name="%views.category.name"
            id="org.eclipse.pde.runtime">
      </category>
      <view
            name="%views.registry.name"
            icon="icons/full/view16/registry.gif"
            category="org.eclipse.pde.runtime"
            class="org.eclipse.pde.internal.runtime.registry.RegistryBrowser"
            id="org.eclipse.pde.runtime.RegistryBrowser">
      </view>
      <view
            name="%views.errorLog.name"
            icon="icons/full/view16/error_log.gif"
            category="org.eclipse.pde.runtime"
            class="org.eclipse.pde.internal.runtime.logview.LogView"
            id="org.eclipse.pde.runtime.LogView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            objectClass="org.eclipse.pde.internal.runtime.logview.LogEntry"
            name="%propertyPages.details.name"
            class="org.eclipse.pde.internal.runtime.logview.LogEntryPropertyPage"
            id="org.eclipse.pde.runtime.StatusDetails">
      </page>
      <page
            objectClass="org.eclipse.pde.internal.runtime.logview.LogEntry"
            name="%propertyPages.session.name"
            class="org.eclipse.pde.internal.runtime.logview.LogSessionPropertyPage"
            id="org.eclipse.pde.runtime.StatusSession">
      </page>
   </extension>

</plugin>
